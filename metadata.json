{
  "name": "theforeman-foreman_network",
  "version": "0.1.0",
  "author": "theforeman",
  "summary": "network configuration",
  "license": "GPLv3+",
  "source": "git://github.com/theforeman/puppet-foreman_network",
  "project_page": "https://github.com/theforeman/puppet-foreman_network",
  "issues_url": "https://github.com/theforeman/puppet-foreman_network/issues",
  "description": "Module for configuring network, firewall and ntpd",
  "tags": [
    "foreman",
    "network"
  ],
  "requirements": [
